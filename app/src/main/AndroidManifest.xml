        android:supportsRtl="true"
        android:theme="@style/Theme.Ejemplosfirebase2223"
        tools:targetApi="31">
        <activity
            android:name=".AddAlumnoActivity"
            android:exported="false" />
        <activity
            android:name=".MostrarAlumnosFirebaseUI"
            android:exported="false" />
        <activity
            android:name=".DetallesAlumnoActivity"
            android:exported="false" />
        <activity
            android:noHistory = "false"
            android:name=".MostrarAlumnos"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>